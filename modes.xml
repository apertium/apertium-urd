<?xml version="1.0" encoding="UTF-8"?>
<modes>

  <mode name="urd-morph" install="yes">
    <pipeline>
      <program name="lt-proc -w">
        <file name="urd.automorf.bin"/>
      </program>
    </pipeline>
  </mode>

  <mode name="urd-gener" install="yes">
    <pipeline>
      <program name="lt-proc -g">
        <file name="urd.autogen.bin"/>
      </program>
    </pipeline>
  </mode>

  <mode name="urd-disam" install="yes">
    <pipeline>
      <program name="lt-proc -w">
        <file name="urd.automorf.bin"/>
      </program>
      <program name="cg-proc -w">
        <file name="urd.rlx.bin"/>
      </program>
    </pipeline>
  </mode>

  <mode name="urd-disam-debug">
    <pipeline>
      <program name="lt-proc -w">
        <file name="urd.automorf.bin"/>
      </program>
      <program name="cg-conv -a -l" />
      <program name="vislcg3 --trace -g">
        <file name="apertium-urd.urd.rlx"/>
      </program>
    </pipeline>
  </mode>


</modes>
